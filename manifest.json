{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.1.0",
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "author": "Sherzod Norkulov",
  "homepage_url": "https://github.com/Sherzod-Norkulov",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
